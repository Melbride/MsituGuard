{
  "version": 2,
  "builds": [
    {
      "src": "crisis_communication/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "crisis_communication/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "crisis_communication.settings",
    "PYTHONPATH": "$PYTHONPATH:."
  }
}